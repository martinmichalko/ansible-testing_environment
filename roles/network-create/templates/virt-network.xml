<network>
  <name>{{ project }}.{{ item.key }}</name>
  <forward mode='{{ forward_mode }}'/>
  <bridge name='{{ project }}.{{ item.key }}' stp='on' delay='0'/>
  <virtualport type='openvswitch'>
  </virtualport>
  <ip address='{{ item.value.gateway }}' netmask='{{ item.value.netmask }}'>
     <dhcp>
       <range start='{{ item.value.dhcp_start }}' end='{{ item.value.dhcp_end }}'/>
     </dhcp>
  </ip>
</network>
